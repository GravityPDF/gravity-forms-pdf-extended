{
  "name": "gravitypdf/gravitypdf",
  "license": "GPL-2.0-or-later",
  "homepage": "https://gravitypdf.com",
  "scripts": {
    "compat": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs --standard=phpcompat.xml.dist",
    "format": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf",
    "lint": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
    "lint:errors": "@lint -n",
    "prefix": "bash ./bin/vendor-prefix.sh"
  },
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "GravityPDFPlugin",
    "platform": {
      "php": "7.3.0"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/GravityPDF/url-signer"
    }
  ],
  "require": {
    "php": ">=7.3",
    "mpdf/mpdf": "dev-master#0db129a868557016614132e2adc67f9e4c048d5e",
    "monolog/monolog": "^2.1.0",
    "spatie/url-signer": "^1.1",
    "mpdf/qrcode": "^1.0",
    "arthurkushman/query-path": "^3.1",
    "gravitypdf/upload": "^3.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "wp-coding-standards/wpcs": "~2.3.0",
    "phpcompatibility/phpcompatibility-wp": "*",
    "roave/security-advisories": "dev-master",
    "yoast/phpunit-polyfills": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "GFPDF\\": "src/",
      "Psr\\Http\\Message\\": "vendor/psr/http-message/src",
      "Psr\\Log\\": "vendor/psr/log/Psr/Log"
    },
    "classmap": [
      "vendor_prefixed/"
    ]
  }
}
